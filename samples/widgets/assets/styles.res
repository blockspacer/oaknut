{
   font-size: 14dp
   Window: {
   	default-width:300dp
   	default-height:348dp
    background-color: #eff6f6
   }

  material_design: {
    size: wrap
    background: {
      @default: {
        corner-radius: 4dp
        fill: #6221ea
      }
      @pressed: {
        corner-radius: 4dp
        fill: #ea2162
      }
    }
    forecolor: white
    elevation: {
      @default: 2dp
      @pressed: 8dp
    }
    ink: #60FFFFFF
  }


}
