    "index":{
        "class":"BNVerticalStack",
        "background":"window"
    },
    "primaryItem":{
        "limit":1,
        "secondary":"$secondary",
        "cellClass":"BNCellItemTopStory",
        "H":1,
        "long_timestamp":true
    },
    "normalGroup":{
        "secondary":"$secondary",
        "cellMargins":"{1,1,1,1}",
        "cellClass":"BNCellItemTopStory",
        "maxTopics":1,
    },
    "featureGroup":{
        "$":"normalGroup",
        "secondary":"$secondary",
        "imagePosition":"top"
        "summaries":true,
        "cellClass":"BNCellItemFeature",
    },
    "featureGroup_2col":{
        "$":"featureGroup",
        "mode!=compact":{
            "cellsPerRow":2,
            "H":2
        }
    },
    "featureItem":{
        "$":"featureGroup",
        "limit":1,
        "imageWidth":0.6666
    },
    "featureItemLarge":{
        "$":"featureGroup",
        "cellClass":"BNCellItemFeatureLarge",
        "limit":1,
        "weight":2
    },
    "featureItemSmall":{
        "$":"featureGroup",
        "limit":1,
        "weight":1
    },
    "digestGroup":{
        "secondary":"$secondary",
        "limit":2,
        "cellClass":"BNCellItemDigest",
        "cellMargins":"{0.25,1,0.25,1}",
    },
    "twoColumns":{
        "class":"BNHorizontalLinearLayout",
        "padding":"{0,8,0,8}",
        "modules":["$1", "$2"]
    },
    "avCarousel":{
        "background":"#000",
        "cellMargins":"{1,1,1,1}",
        "arrange": "horizontal",
        "secondary":"$st_av_carousel",
        "orderBy":"avcarousel",
        "cellClass":"BNCellItemAV",
        "orientation":"portrait",
        "lines":3,
        "format":[".format.video",".format.audio"]
    },
    "photosGroup":{
        "$secondary":"$st_in_pictures",
        "limit":2,
        "mode==compact":{
            "$":"normalGroup",
            "summaries":true,
            "hideTopics":true
        },
        "mode!=compact":{
            "$":"normalGroup",
            "cellClass":"BNCellItemPhotoGallery",
            "maxImages":2
        },
        "mode==tablet":{
            "maxImages":3
        }
    },
    "mostPopularGroup":{
        "cellClass":"BNCellItemMostPopular",
        "limit":10,
        "primary":[".item"],
        "padding":"{0,0,0,0}",
        "orientation":"portrait",
        "H":1
    },
    "mostReadGroup":{
        "$":"mostPopularGroup",
        "secondary":[".most_popular.most_read"],
        "format":[".format.textual"],
        "inverseColorScheme":"NO"
    },
    "mostWatchedGroup":{
        "$":"mostPopularGroup",
        "secondary":[".most_popular.most_watched"],
        "format":[".format.video"],
        "inverseColorScheme":true
    },
    "myNewsByTimeGroup":{
        "$":"otherItems",
        "offset":0,
        "limit":-1,
        "primary":[".item"],
        "orderBy":"time",
        "orientation":"portrait",
        "H":1
    },
    "myNewsByTopicTitle":{
        "class":"BNContentTitleModule",
        "padding":"{24,16,4,8}"
    },
    "myNewsByTopicOldStyle":{
        "$":"normalGroup",
        "primary":[".item"],
        "limit":4,
        "orientation":"portrait",
        "cellsPerRow":2,
        "hideTopics":true,
        "summaries":false,
    },
    "myNewsByTopicNonCarousel":{
        "$":"normalGroup",
        "primary":[".item"],
        "limit":3,
        "hideTopics":true,
        "summaries":false,
    },
    "myNewsByTopicCarousel":{
        "class":"BNHorizontalStack",
        "background":"#E6E6E6",
        "modules":[{
            "limit":12,
            "dummyEndCell":"true",
            "offset":0,
            "primary":[".item"],
            "orderBy":"avcarousel",
            "cellClass":"BNCellItemAV",
            "orientation":"portrait",
            "lines":3,
            "hideTopics":true,
            "openAllItemsOnTap":true,
            "tinyTimestamps":true
        }]
    },

    "secondaryItems":{
        "$":"normalGroup",
        "secondary":"$secondary",
        "limit":2,
        "offset":1
    },
    "otherItems":{
        "$":"normalGroup",
        "secondary":"$secondary",
        "offset":3
    },
    "featuresSection":{
        "class":"BNVerticalLinearLayout",
        "modules":[
           {"$":"featureItem", "imagePosition":"left","offset":0},
           {"$":"featureItem", "imagePosition":"right", "offset":1},
           {"$":"featureGroup_2col", "offset":2, "limit":10}
        ]
    },
    "itemPrimaryMedia":{
        "cellClass":"BNCellMedia",
        "limit":1,
        "primary":[".image", ".video", ".audio"],
        "secondary":[".placement.primary"],
        "parallax":true,
        "imageAspect":0.5625
    },
    "itemHeadline":{
        "cellClass":"BNCellHeadline"
    },
    "itemByline":{
        "cellClass":"BNCellByline",
        "primary":[".person"],
        "secondary":[".byline"],
        "limit":1
    },
    "itemTopics":{
        "cellClass":"BNCellTopics"
    },
    "itemBody":{
        "class":"BNCellsModuleArticleBody",
    }
}
